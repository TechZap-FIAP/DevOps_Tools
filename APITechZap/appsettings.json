{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "OracleConnection": "$(CONNECTION_STRING_ORACLE)"
  },
  "Authentication": {
    "TokenUri": "https://identitytoolkit.googleapis.com/v1/accounts:signInWithPassword?key=$(API_KEY_FIREBASE)"
  },
  "AiService": {
    "Uri": "https://api.openai.com/v1/chat/completions",
    "ApiKey": "$(API_KEY_OPENAI)"
  }
}